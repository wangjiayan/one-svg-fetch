{
  "name": "one-svg-tool",
  "version": "0.0.7-alpha.0",
  "description": "> TODO: description",
  "author": "wangjiayan <765390818@qq.com>",
  "homepage": "https://github.com/wangjiayan/one-svg-feth#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangjiayan/one-svg-feth.git"
  },
  "scripts": {
    "build:bundle": "sudo rm -rf dist && node_modules/.bin/rollup --c ./build/rollup.config.bundle.js",
    "clear": "rimraf ./dist && rimraf ./libs",
    "compile:typings": "tsc --project ./tsconfig.typings.json --outDir ./libs --declaration",
    "compile:code": "yarn clear && cross-env npx babel ./src --config-file ./.babelrc.js --out-dir libs --extensions .ts",
    "compile": "concurrently \"yarn compile:code\"",
    "watch:compile:code": "cross-env npx babel ./src --config-file ./.babelrc.js --out-dir libs --extensions .ts --skip-initial-build --watch",
    "watch:compile:typings": "tsc --project ./tsconfig.typings.json --outDir ./libs --declaration --watch",
    "watch:compile": "concurrently \"yarn watch:compile:code\"",
    "fetch": "node test/fetchSVG.js",
    "start": "yarn watch:compile",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-optional-chaining": "7.16.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/node": "^16.11.10",
    "babel-plugin-transform-define": "^2.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  },
  "bugs": {
    "url": "https://github.com/wangjiayan/one-svg-feth/issues"
  },
  "gitHead": "9c65801790aacb709ca75d77002db5540d8d476a"
}
