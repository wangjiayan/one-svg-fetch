{
  "name": "figma-svg-fetch",
  "version": "1.0.0",
  "description": "一个从figma上拉取icon的工具",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start":"lerna run start --scope=tools",
    "compile":"lerna run compile --scope=tool",
    "example":"lerna run fetch --scope=example"
  },
  "hooks": {
    "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
  },
